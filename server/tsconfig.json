{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "../",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "noEmit": false,
    "declaration": false,
    "sourceMap": true,
    "allowJs": false,
    "allowImportingTsExtensions": false,
    "types": ["node"]
  },
  "include": [
    "src/**/*.ts",
    "../lib/leaderboard.ts",
    "../types.ts"
  ]
}
